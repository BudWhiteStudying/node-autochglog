{
  "name": "node-autochglog",
  "version": "1.0.0",
  "description": "Yet another git changelog generator, for Node.",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"No test specified, skipping\"",
    "build": "npx tsc",
    "lint": "npx eslint --fix .",
    "prepare": "husky install",
    "local-test": "node dist/index.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@types/node": "^22.10.1",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.13.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.10",
    "prettier": "3.4.2",
    "typescript-eslint": "^8.17.0"
  }
}
